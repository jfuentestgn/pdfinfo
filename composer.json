    "type": "library",
    "require": {
        "symfony/process": "^3.0",
        "spatie/pdf-to-text": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Fuentes",
            "email": "jfuentestgna@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
